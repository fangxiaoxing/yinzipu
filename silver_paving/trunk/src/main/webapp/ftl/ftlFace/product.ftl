    <script>
        function checkSubmit() {
            var btnSubmit = $("#submit-invest");
            var delAttr = function () {
                setTimeout(function () {
                    btnSubmit.removeAttr("disabled");
                }, 250);
            }
            var $amount = $("#investInput");
            var amount = $amount.val();
            var min = $amount.attr("data-min") * 1;
            var max = $amount.attr("data-limit") * 1;
            if (amount == "") {
                YZP.alert("请输入投资金额");
                $amount.focus();
                delAttr();
                return false;
            } else {
                if (isNaN(amount)) {
                    YZP.alert("投资金额必须为数字");
                    $amount.focus();
                    $amount.val();
                    delAttr();
                    return false;
                } else if (amount > parseFloat(amount).toFixed(0)) {
                    YZP.alert("投资金额必须是1的倍数");
                    $amount.focus();
                    $amount.val();
                    delAttr();
                    return false;
                } else if (!isNaN(min) & amount * 1 < min) {
                    YZP.alert(min + "元起投");
                    $amount.focus();
                    $amount.val("");
                    delAttr();
                    return false;
                } else if (!isNaN(max) & amount * 1 > max) {
                    YZP.alert("最大可投金额" + max + "元");
                    $amount.focus();
                    $amount.val(max);
                    delAttr();
                    return false;
                }
            }
        }
    </script>

   


